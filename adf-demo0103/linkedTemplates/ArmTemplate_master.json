{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-demo0103"
		},
		"AdvWorksDb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb'"
		},
		"AdvWorksDb1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb1'"
		},
		"AdvWorksDb2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AdvWorksDb2'"
		},
		"BlobStorageforCustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageforCustomer'"
		},
		"BlobStorageforProducts_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorageforProducts'"
		},
		"AdvWorksDb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eshaa.database.windows.net"
		},
		"AdvWorksDb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "workspace"
		},
		"AdvWorksDb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eshaa"
		},
		"AdvWorksDb1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eshaa.database.windows.net"
		},
		"AdvWorksDb1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "workspace"
		},
		"AdvWorksDb1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eshaa"
		},
		"AdvWorksDb2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "eshaa.database.windows.net"
		},
		"AdvWorksDb2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "workspace"
		},
		"AdvWorksDb2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "eshaa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-demo0103_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdvWorksDb_password": {
						"value": "[parameters('AdvWorksDb_password')]"
					},
					"AdvWorksDb1_password": {
						"value": "[parameters('AdvWorksDb1_password')]"
					},
					"AdvWorksDb2_password": {
						"value": "[parameters('AdvWorksDb2_password')]"
					},
					"BlobStorageforCustomer_connectionString": {
						"value": "[parameters('BlobStorageforCustomer_connectionString')]"
					},
					"BlobStorageforProducts_connectionString": {
						"value": "[parameters('BlobStorageforProducts_connectionString')]"
					},
					"AdvWorksDb_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_server')]"
					},
					"AdvWorksDb_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_database')]"
					},
					"AdvWorksDb_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb_properties_typeProperties_userName')]"
					},
					"AdvWorksDb1_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_server')]"
					},
					"AdvWorksDb1_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_database')]"
					},
					"AdvWorksDb1_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb1_properties_typeProperties_userName')]"
					},
					"AdvWorksDb2_properties_typeProperties_server": {
						"value": "[parameters('AdvWorksDb2_properties_typeProperties_server')]"
					},
					"AdvWorksDb2_properties_typeProperties_database": {
						"value": "[parameters('AdvWorksDb2_properties_typeProperties_database')]"
					},
					"AdvWorksDb2_properties_typeProperties_userName": {
						"value": "[parameters('AdvWorksDb2_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}